APP_NAME="AI CI/CD Demo"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=ai_cicd_demo
DB_USERNAME=root
DB_PASSWORD=

BROADCAST_DRIVER=log
CACHE_DRIVER=file
FILESYSTEM_DISK=local
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

# AI Pipeline Configuration
AI_TEST_SELECTION_ENABLED=true
AI_TEST_CONFIDENCE_THRESHOLD=0.75
AI_TEST_MAX_TESTS=100

AI_FAILURE_PREDICTION_ENABLED=true
AI_PREDICTION_CONFIDENCE=0.7
AI_AUTO_UPDATE_MODEL=true

AI_PARALLEL_TESTS=true
AI_PARALLEL_PROCESSES=4

AI_PIPELINE_DEBUG=false
AI_SIMULATE=false

# GitHub Integration (for demo)
GITHUB_TOKEN=
GITHUB_REPOSITORY=

# Slack Notifications (optional)
SLACK_WEBHOOK_URL=
